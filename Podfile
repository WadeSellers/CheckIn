# Uncomment this line to define a global platform for your project
platform :ios, '8.0'
# Uncomment this line if you're using Swift
use_frameworks!

#ignore all warnings from all pods 
inhibit_all_warnings!

target 'iOS-CheckInApp' do
  pod 'NSLogger'
  pod 'ObjectiveDDP'
  pod 'AmazonS3RequestManager', '~> 0.10'
  pod 'RealmSwift'
  pod 'Fabric'
  pod 'Crashlytics'
end

target 'iOS-CheckInAppDev' do
  pod 'NSLogger'
  pod 'ObjectiveDDP'
  pod 'AmazonS3RequestManager', '~> 0.10'
  pod 'RealmSwift'
  pod 'Fabric'
  pod 'Crashlytics'
end


target 'iOS-CheckInAppTests' do
  pod 'NSLogger'
  pod 'ObjectiveDDP'
end

target 'iOS-CheckInAppUITests' do
  pod 'NSLogger'
end

